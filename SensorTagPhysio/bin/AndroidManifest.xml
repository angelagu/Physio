<?xml version="1.0" encoding="utf-8"?>
<manifest android:versionCode="10" android:versionName="1.10" package="ti.android.ble.sensortag"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-feature android:name="android.hardware.bluetooth_le" android:required="true" />
    <uses-feature android:name="android.hardware.bluetooth.ble" android:required="true" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <application android:theme="@*android:style/Theme.Holo.Light" android:label="@string/app_name" android:icon="@drawable/tible" android:debuggable="true" android:allowBackup="false">
        <service android:name="ti.android.ble.common.BluetoothLeService" android:enabled="true" />
        <activity android:label="@string/app_name" android:name="MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="DeviceActivity" android:screenOrientation="portrait" android:parentActivityName="MainActivity" />
        <activity android:name="ServiceActivity" android:screenOrientation="portrait" android:parentActivityName="DeviceActivity" />
        <activity android:name="FwUpdateActivity" android:screenOrientation="portrait" android:parentActivityName="DeviceActivity" />
        <activity android:name="FileActivity" android:screenOrientation="portrait" android:parentActivityName="FwUpdateActivity" />
        <activity android:label="Preferences" android:name="PreferencesActivity" android:screenOrientation="portrait" android:parentActivityName="DeviceActivity" />
    </application>
</manifest>